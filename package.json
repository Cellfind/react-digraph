{
  "name": "@cellfind/react-digraph",
  "description": "directed graph react component",
  "version": "1.0.2",
  "keywords": [
    "babel",
    "es6",
    "d3",
    "react",
    "graph",
    "digraph"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Cellfind/react-digraph.git"
  },
  "bugs": {
    "url": "https://github.com/Cellfind/react-digraph/issues/new",
    "email": "clayton.barnard@cellfind.co.za"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "license": "MIT",
  "main": "dist/main.min.js",
  "types": "./typings/index.d.ts",
  "peerDependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "dependencies": {
    "d3": "^5.12.0",
    "dagre": "^0.8.4",
    "fast-deep-equal": "^2.0.1",
    "html-react-parser": "^0.9.1",
    "kld-affine": "^2.1.1",
    "kld-intersections": "^0.6.0",
    "line-intersect": "^2.2.3",
    "svg-intersections": "^0.4.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.7.2",
    "@babel/node": "^7.6.3",
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-destructuring": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.6.3",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-react": "^1.0.0",
    "brace": "^0.11.1",
    "browserify": "^14.4.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^1.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.5.1",
    "eslint-config-fusion": "^6.0.4",
    "eslint-config-prettier": "^6.4.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-cup": "^2.0.2",
    "eslint-plugin-es6-recommended": "^0.1.2",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-import-order": "^2.1.4",
    "eslint-plugin-jest": "^22.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "flow-bin": "^0.86.0",
    "husky": "^2.4.0",
    "jest": "^24.9.0",
    "jsdom": "^11.12.0",
    "lint-staged": "^8.2.0",
    "live-server": "^1.2.0",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.5",
    "opn-cli": "3.1.0",
    "prettier": "^1.12.0",
    "prop-types": "^15.6.0",
    "react": "^16.4.2",
    "react-ace": "^6.1.4",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "svg-inline-loader": "^0.8.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9"
  },
  "scripts": {
    "build": "webpack",
    "build:prod": "webpack --config webpack.prod.js",
    "clean": "rimraf ./dist",
    "watch": "webpack --watch",
    "build-css": "node-sass --include-path scss src/styles/main.scss dist/styles/main.css && node-sass --include-path scss src/examples/app.scss dist/examples/app.css",
    "cover": "npm run test",
    "example": "npm run serve",
    "flow": "flow .",
    "jenkins-install": "npm install",
    "jenkins-jshint": "npm run lint -- --o=jshint.xml --f=checkstyle",
    "jenkins-test": "npm run jenkins-jshint && npm run test",
    "live-server": "live-server ./dist --entry-file=./index.html",
    "live-serve": "npm-run-all --parallel watch live-server",
    "lint": "eslint src",
    "lint-fix": "eslint --fix src",
    "precommit": "lint-staged && npm run test",
    "prefast-test": "npm run prepublish",
    "prepublish": "npm run package",
    "serve": "npm run live-serve",
    "test": "jest",
    "test:debug": "node --inspect node_modules/.bin/jest --watch --runInBand",
    "view-cover": "npm run cover -- --report=html && opn ./coverage/index.html",
    "package": "npm-run-all clean lint build build:prod",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "jest": {
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transformIgnorePatterns": [
      "node_modules"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)"
    ],
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage",
    "collectCoverageFrom": [
      "**/src/**/*.{js,ts,tsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/*.d.ts",
      "!**/examples/**"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html",
      "cobertura"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest-setup.js",
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/__mocks__/styles.mock.js",
      "@fortawesome/fontawesome-free/svgs/solid/expand.svg": "<rootDir>/__mocks__/icon.mock.js"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
